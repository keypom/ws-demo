{
  "name": "hello-near-js",
  "version": "1.0.0",
  "license": "(MIT AND Apache-2.0)",
  "scripts": {
    "start:all": "yarn start:coin-flip && yarn start:counter && yarn start:donation && yarn start:guest-book && yarn start:hello-near",
    "start:coin-flip": "cd coin-flip && npm run start",
    "start:counter": "cd counter && npm run start",
    "start:donation": "cd donation && npm run start",
    "start:guest-book": "cd guest-book && npm run start",
    "start:hello-near": "cd hello-near && npm run start",
    "start:near-social": "yarn build:near-social && cd near-social-demo/viewer && npm run start",
    "build:near-social": "cd near-social-demo/VM && yarn build",

    "install-deps:all": "yarn install-deps:coin-flip && yarn install-deps:counter && yarn install-deps:donation && yarn install-deps:guest-book && yarn install-deps:hello-near",
    "install-deps:coin-flip": "cd coin-flip && npm install",
    "install-deps:counter": "cd counter && npm install",
    "install-deps:donation": "cd donation && npm install",
    "install-deps:guest-book": "cd guest-book && npm install",
    "install-deps:hello-near": "cd hello-near && npm install",
    "install-deps:near-social": "yarn install-deps:near-social:viewer && yarn install-deps:near-social:vm",
    "install-deps:near-social:viewer": "cd near-social-demo/viewer && yarn install",
    "install-deps:near-social:vm": "cd near-social-demo/vm && yarn install",

    "create-trial-drop": "node ./scripts/create-trial-drop.js"
  },
  "devDependencies": {
    "near-cli": "^3.3.0",
    "keypom-js": "^1.4.0-rc.2"
  },
  "dependencies": {}
}